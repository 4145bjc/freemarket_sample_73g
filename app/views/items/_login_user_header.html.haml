.login
  .login__main
    .login__main__inner
      .login__main__inner__icon
        = link_to image_tag "logo/logo.png"
      .login__main__inner__key
        %input{placeholder: "キーワードから探す"}
          .login__main__inner__key__search
            = link_to image_tag "icon/icon-search 1.png"
    .login__main__bottom
      .login__main__bottom__left
        %ul.login__main__bottom__left__ul
          %li.login__main__bottom__left__ul__list カテゴリー
          %li.login__main__bottom__left__ul__list ブランド
      .login__main__bottom__right
        %ul.login__main__bottom__right__ul
          %li.login__main__bottom__right__ul__list 
        = link_to "マイページ", users_path
